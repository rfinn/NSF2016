	Doi = {10.1051/0004-6361:20041316},
	Eprint = {astro-ph/0409110},
	Journal = {\aap},
	Keywords = {galaxies: spiral, galaxies: ISM, stars: formation, infrared: ISM},
	Month = dec,
	Pages = {409-423},
	Title = {{Mid-IR emission of galaxies in the Virgo cluster and in the Coma supercluster. IV. The nature of the dust heating sources}},
