	Author = {{Jaff{\'e}}, Y.~L. and {Arag{\'o}n-Salamanca}, A. and {Kuntschner}, H. and {Bamford}, S. and {Hoyos}, C. and {De Lucia}, G. and {Halliday}, C. and {Milvang-Jensen}, B. and {Poggianti}, B. and {Rudnick}, G. and {Saglia}, R.~P. and {Sanchez-Blazquez}, P. and {Zaritsky}, D.},
