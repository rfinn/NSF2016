	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
	Month = sep,
	Title = {{The Confinement of Star-Forming Galaxies into a Main Sequence through Episodes of Gas Compaction, Depletion, and Replenishment}},
	Year = 2015,
	Bdsk-File-1 = {},
