	Keywords = {galaxies: clusters: general, galaxies: evolution, galaxies: formation},
	Month = feb,
	Pages = {L1-L6},
	Primaryclass = {astro-ph.CO},
	Title = {{Comparing the Relation Between Star Formation and Galaxy Mass in Different Environments}},
