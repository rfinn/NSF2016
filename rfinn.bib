	Eprint = {astro-ph/0507605},
	Journal = {\aap},
	Keywords = {galaxies: clusters: general, galaxies: abundances, galaxies: interactions, galaxies: ISM, X-rays: galaxies: clusters},
	Month = jun,
	Pages = {795-802},
	Title = {{Enrichment of the ICM of galaxy clusters due to ram-pressure stripping}},
