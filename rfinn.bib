	Journal = {\apj},
	Keywords = {Galaxies: Clusters: General, Galaxies: Evolution, Galaxies: Stellar Content},
	Month = sep,
	Pages = {888-904},
	Title = {{The Relation between Star Formation, Morphology, and Local Density in High-Redshift Clusters and Groups}},
	Volume = 684,
	Year = 2008,
