	Journal = {\apj},
	Keywords = {ISM: Dust, Extinction, Galaxies: General, Galaxies: ISM, Infrared: Galaxies, Infrared: ISM},
	Month = sep,
	Pages = {159-168},
	Title = {{The Infrared Spectral Energy Distribution of Normal Star-forming Galaxies: Calibration at Far-Infrared and Submillimeter Wavelengths}},
