	Keywords = {galaxies: clusters: general, galaxies: clusters: individual: RX J1821.6+6827, X-rays: general, X-rays: individuals: RX J1821.6+6827, X-rays: galaxies: clusters, galaxies: intergalactic medium, z08clusters},
	Month = dec,
	Pages = {867-875},
	Title = {{RX J1821.6+6827: A cool cluster at z = 0.81 from the ROSAT NEP survey}},
