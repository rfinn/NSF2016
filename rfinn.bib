	Eprint = {astro-ph/0603826},
	Journal = {\aap},
	Keywords = {galaxies: evolution, galaxies: clusters: individual: Abell 1367, galaxies: interactions, galaxies: intergalactic medium},
	Month = jul,
	Pages = {847-861},
	Title = {{Witnessing galaxy preprocessing in the local Universe: the case of a star-bursting group falling into Abell 1367}},
