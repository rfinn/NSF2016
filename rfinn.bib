	Doi = {10.1086/309323},
	Eprint = {astro-ph/0004078},
	Journal = {\apj},
	Keywords = {Galaxies: Clusters: General, Galaxies: Evolution, Galaxies: Stellar Content, Stars: Formation},
	Month = sep,
	Pages = {113-121},
	Title = {{The Origin of Star Formation Gradients in Rich Galaxy Clusters}},
