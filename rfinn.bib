	Eprint = {1203.0308},
	Journal = {\mnras},
	Keywords = {methods: numerical, galaxies: clusters: intracluster medium, galaxies: interactions},
	Month = may,
	Pages = {1609-1624},
	Title = {{Star formation in ram pressure stripped galactic tails}},
	Volume = 422,
	Year = 2012,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2012.20737.x},
