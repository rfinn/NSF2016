	Author = {{Kauffmann}, G. and {Heckman}, T.~M. and {Tremonti}, C. and {Brinchmann}, J. and {Charlot}, S. and {White}, S.~D.~M. and {Ridgway}, S.~E. and {Brinkmann}, J. and {Fukugita}, M. and {Hall}, P.~B. and {Ivezi{\'c}}, {\v Z}. and {Richards}, G.~T. and {Schneider}, D.~P.},
