	Keywords = {COSMOLOGY: OBSERVATIONS, COSMOLOGICAL: PARAMETERS, X-RAYS: GALAXIES: CLUSTERS},
	Month = mar,
	Pages = {773-790},
	Title = {{A new measurement of the X-ray temperature function of clusters of galaxies}},
