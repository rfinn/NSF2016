	Keywords = {Emission Spectra, Galactic Clusters, Galactic Radiation, H Alpha Line, Spiral Galaxies, Astronomical Spectroscopy, Sky Surveys (Astronomy), Space Observations (From Earth), Star Formation, Stellar Spectra},
	Month = apr,
