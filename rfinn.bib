	Journal = {\apjl},
	Keywords = {Galaxies: Clusters: General, Galaxies: Evolution, Galaxies: Fundamental Parameters},
	Month = oct,
	Pages = {L113-L116},
	Title = {{Spitzer/MIPS 24 {$\mu$}m Observations of Galaxy Clusters: An Increasing Fraction of Obscured Star-forming Members from z = 0.02 to z = 0.83}},
