@article{bahe13,
	Abstract = {In the local Universe, galaxies in groups and clusters contain less gas and are less likely to be forming stars than their field counterparts. This effect is not limited to the central group/cluster regions, but is shown by recent observations to persist out to several virial radii. To gain insight into the extent and cause of this large-scale environmental influence, we use a suite of high-resolution cosmological hydrodynamic simulations to analyse galaxies around simulated groups and clusters of a wide range of mass (log 10Mhost/M&sun; = [13.0, 15.2]). In qualitative agreement with the observations, we find a systematic depletion of both hot and cold gas and a decline in the star-forming fraction of galaxies as far out as ˜5r200 from the host centre. A substantial fraction of these galaxies are on highly elliptical orbits and are not infalling for the first time (˜50 per cent at 2r200, independent of host mass) or are affected by `pre-processing' (less than 10 per cent of galaxies around groups, increasing to ˜50 per cent near a massive cluster), even a combination of these indirect mechanisms does not fully account for the environmental influence, particularly in the case of the hot gas content. Direct ram pressure interaction with an extended gas `halo' surrounding groups and clusters is shown to be sufficiently strong to strip the hot gas atmospheres of infalling galaxies out to ˜5r200. We show that this influence is highly anisotropic, with ram pressure along filaments enhanced by up to a factor of 100 despite significant co-flow of gas and galaxies.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2013MNRAS.430.3017B},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1210.8407},
	Author = {{Bah{\'e}}, Y.~M. and {McCarthy}, I.~G. and {Balogh}, M.~L. and {Font}, A.~S.},
	Date-Added = {2015-08-22 11:26:24 +0000},
	Date-Modified = {2015-08-22 11:26:24 +0000},
	Doi = {10.1093/mnras/stt109},
	Eprint = {1210.8407},
	Journal = {\mnras},
	Keywords = {galaxies: clusters: general, galaxies: evolution, galaxies: haloes, galaxies: interactions, intergalactic medium, galaxies: ISM},
	Month = apr,
	Pages = {3017-3031},
	Title = {{Why does the environmental influence on group and cluster galaxies extend beyond the virial radius?}},
	Volume = 430,
