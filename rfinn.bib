	Keywords = {galaxies: fundamental parameters, galaxies: spiral, methods: statistical, radio lines: galaxies, surveys},
	Month = may,
	Pages = {93},
	Primaryclass = {astro-ph.CO},
	Title = {{H I Content and Optical Properties of Field Galaxies from the ALFALFA Survey. II. Multivariate Analysis of a Galaxy Sample in Low-density Environments}},
