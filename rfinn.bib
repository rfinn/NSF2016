	Journal = {\apj},
	Keywords = {Disk Galaxies, Galactic Evolution, Spiral Galaxies, Star Formation, Stellar Spectrophotometry, Emission Spectra, Galactic Structure, H Alpha Line, Interstellar Gas, Photoionization, Star Distribution, Star Formation Rate, Stellar Models, Ubv Spectra},
	Month = sep,
	Pages = {54-67},
	Title = {{The rate of star formation in normal disk galaxies}},
