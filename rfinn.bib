	Doi = {10.1073/pnas.1407300111},
	Eprint = {1405.0537},
	Journal = {Proceedings of the National Academy of Science},
	Month = jun,
	Pages = {7914-7919},
	Title = {{Gas loss in simulated galaxies as they fall into clusters}},
