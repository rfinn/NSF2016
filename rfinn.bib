	Journal = {\aj},
	Keywords = {Galactic Evolution, Interacting Galaxies, Spiral Galaxies, Star Distribution, Star Formation, Star Formation Rate, Galactic Structure, H Alpha Line, Infrared Astronomy Satellite, Luminosity},
	Month = may,
	Pages = {1011-1023},
	Title = {{The effects of interactions on spiral galaxies. II - Disk star-formation rates}},
